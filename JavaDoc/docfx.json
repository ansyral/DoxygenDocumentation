{
	"build": {
		"content": [{
			"files": [
				"api/**/*.yml",
				"toc.yml",
				"api/*.md"
			],
			"exclude": [
				"java/**",
				"**/bin/**",
				"**/obj/**"
			]
		}],
		"resource": [{
			"files": [
				"**/*.png",
				"**/*.jpg"
			],
			"exclude": [
				"**/bin/**",
				"**/obj/**",
				"java/**"
			]
		}],
		"overwrite": "apidoc/*.md",
		"globalMetadata": {
			"breadcrumb_path": "/java/toc.json"
		},
		"dest": "java",
		"template": [
			"docs.html"
		]
	}
}
